      "name": "capture - add ingredient",
      "type": "Capture",
      "command": true,
      "appendLink": false,
      "captureTo": "",
      "captureToActiveFile": true,
      "createFileIfItDoesntExist": {
        "enabled": false,
        "createWithTemplate": false,
        "template": ""
      },
      "format": {
        "enabled": true,
        "format": "```js quickadd\nconst dv = app.plugins.plugins.dataview.api;\nconst tp = app.plugins.plugins['templater-obsidian'].templater.current_functions_object;\n\nconst func = (item) => `${item.file.name}`;\n\nconst ingredient = (await tp.system.suggester(func, dv.pages('#ingredient'), false, \"none\")).file.link;\n\nconst amount = (await tp.system.prompt(\"amount\", \"\"));\nreturn `Ingredient:: ${ingredient} ${amount}`\n```\n"
      },
      "insertAfter": {
        "enabled": true,
        "after": "## Ingredients",
        "insertAtEnd": true,
        "considerSubsections": false,
        "createIfNotFound": false,
        "createIfNotFoundLocation": "top"
